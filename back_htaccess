    RewriteRule ^rebuilding/corso$ rebuilding/rebuilding_corsofad.php
    RewriteRule ^rebuilding/toolkit$ rebuilding/rebuilding_toolkit.php
    RewriteRule ^rebuilding/rendicontazione$ rebuilding/rebuilding_rendicontazione.php
    RewriteRule ^rebuilding/flussofinanziario$ rebuilding/rebuilding_flussofinanziario.php
    RewriteRule ^rebuilding/flussofinanziari-xls$ rebuilding/rebuilding_flussofinanziari_esporta.php
